{
  "name": "street-cast-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "db:generate": "node scripts/check-env.js && prisma generate",
    "db:push": "node scripts/check-env.js && prisma db push",
    "db:studio": "node scripts/check-env.js && prisma studio",
    "db:seed": "node scripts/check-env.js && node scripts/seed.js",
    "setup": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "next": "15.5.4",
    "prisma": "^5.22.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
